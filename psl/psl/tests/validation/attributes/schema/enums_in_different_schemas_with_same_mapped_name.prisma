generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["multiSchema"]
}

datasource db {
  provider   = "postgresql"
  url        = env("TEST_DATABASE_URL")
  namespaces = ["base", "transactional"]
}

enum Color {
    RED
    GREEN
    BLUE

    @@map("attribute")
    @@namespace("base")
}

enum Size {
    SMALL
    MEDIUM
    LARGE
    VENTI

    @@map("attribute")
    @@namespace("transactional")
}
