datasource testds {
    provider = "postgresql"
    url = env("TEST_DATABASE_URL")
    namespaces = ["public"]
}

generator js {
    provider = "prisma-client-js"
    previewFeatures = ["multiSchema"]
}

model Test {
  id Int @id

  @@namespace("nonpublic")
}

enum Language {
  English
  Foreign

  @@namespace("nonpublic")
}



