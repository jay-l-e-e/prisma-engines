datasource db {
  provider   = "postgresql"
  url        = env("DATABASE_URL")
  namespaces = ["A", "B"]
}

generator js {
  provider = "prisma-client-js"
  previewFeatures = ["views", "multiSchema"]
}

model A {
  id Int @id

  @@namespace("B")
}

view Mountain {
  id Int @unique

  @@namespace("A")
}
