datasource pg {
  provider   = "postgresql"
  url        = "postgresql://"
  namespaces = ["public", "test"]
}

generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["multiSchema"]
}

model A {
  id Int @id

  @@namespace("public")
}

model B {
  id Int @id

  @@namespace("test")
}
